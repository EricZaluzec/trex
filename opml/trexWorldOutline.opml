			<outline text="&lt;%outlineEmbedCode%&gt;">

				<outline text="&lt;script src=&quot;http://outliner.smallpicture.com/concord.js&quot; /&gt;&lt;/script&gt;"/>

				<outline text="&lt;script src=&quot;http://outliner.smallpicture.com/utils.js&quot;&gt;&lt;/script&gt;"/>

				<outline text="&lt;div class=&quot;divTrexEmbeddedOutlineContainer&quot;&gt;">

					<outline text="&lt;div id=&quot;idAboveOutlineInfo&quot; class=&quot;divAboveOutlineInfo&quot;&gt;">

						<outline text="&lt;div id=&quot;idViewSource&quot; class=&quot;divInfoRight&quot;&gt;&lt;/div&gt;"/>

						<outline text="&lt;div id=&quot;idOutlineTitle&quot; class=&quot;divInfoLeft divOutlineTitle&quot;&gt;&lt;/div&gt;"/>

						<outline text="&lt;/div&gt;"/>

						</outline>

					<outline text="&lt;div id=&quot;idTrexEmbeddedOutliner&quot; class=&quot;divTrexEmbeddedOutliner&quot;&gt;&lt;/div&gt;"/>

					<outline text="&lt;div id=&quot;idBelowOutlineInfo&quot; class=&quot;divBelowOutlineInfo&quot;&gt;">

						<outline text="&lt;div class=&quot;divInfoRight&quot;&gt;">

							<outline text="&lt;i class=&quot;icon-heart&quot; style=&quot;color: red; opacity: 0.6; font-size: 0.9em;&quot;&gt;&lt;/i&gt; "/>

							<outline text="&lt;a href=&quot;http://fargo.io/&quot; target=&quot;_blank&quot;&gt;Fargo&lt;/a&gt;"/>

							<outline text="&lt;/div&gt;"/>

							</outline>

						<outline text="&lt;div class=&quot;divInfoLeft&quot;&gt;&lt;/div&gt;"/>

						<outline text="&lt;/div&gt;"/>

						</outline>

					<outline text="&lt;/div&gt;"/>

					</outline>

				<outline text="&lt;script&gt;">

					<outline text="var font = &quot;&lt;%font%&gt;&quot;, fontSize = &lt;%fontSize%&gt;, lineHeight = &lt;%lineHeight%&gt;;"/>

					<outline text="var opmlUrl = &quot;&lt;%opmlUrl%&gt;&quot;;"/>

					<outline text="function opKeystrokeCallback () {">

						<outline text="}"/>

						</outline>

					<outline text="function setPrefs () {">

						<outline text="$(&quot;#idTrexEmbeddedOutliner&quot;).concord ({">

							<outline text="&quot;prefs&quot;: {">

								<outline text="&quot;outlineFont&quot;: font, "/>

								<outline text="&quot;outlineFontSize&quot;: fontSize, "/>

								<outline text="&quot;outlineLineHeight&quot;: lineHeight,"/>

								<outline text="&quot;renderMode&quot;: true,"/>

								<outline text="&quot;readonly&quot;: true,"/>

								<outline text="&quot;typeIcons&quot;: appTypeIcons"/>

								<outline text="},"/>

								</outline>

							<outline text="&quot;callbacks&quot;: {">

								<outline text="&quot;opExpand&quot;: opExpandCallback,"/>

								<outline text="&quot;opKeystroke&quot;: opKeystrokeCallback"/>

								<outline text="}"/>

								</outline>

							<outline text="});"/>

							</outline>

						<outline text="}"/>

						</outline>

					<outline text="function getOutline (opmlUrl) {">

						<outline text="var jxhr = $.ajax ({">

							<outline text="url: opmlUrl,"/>

							<outline text="dataType: &quot;text&quot;,"/>

							<outline text="timeout: 30000"/>

							<outline text="})"/>

							</outline>

						<outline text=".success (function (data, status, xhr) {">

							<outline text="if (xhr.status==200) {">

								<outline text="var title;"/>

								<outline text="$(&quot;#idTrexEmbeddedOutliner&quot;).concord ().op.xmlToOutline (data);"/>

								<outline text="title = $(&quot;#idTrexEmbeddedOutliner&quot;).concord ().op.getTitle ();"/>

								<outline text="$(&quot;#idOutlineTitle&quot;).html (title);"/>

								<outline text="$(&quot;#idViewSource&quot;).html (&quot;&lt;a href='&quot; + opmlUrl + &quot;'&gt;view  source&lt;/a&gt;&quot;);"/>

								<outline text="}"/>

								</outline>

							<outline text="})"/>

							</outline>

						<outline text=".error (function (status, textStatus, errorThrown) {">

							<outline text="console.log (&quot;checkOutline: error == &quot; + errorThrown + &quot;, status == &quot; + textStatus);"/>

							<outline text="});"/>

							</outline>

						<outline text="}"/>

						</outline>

					<outline text="$(document).ready (function () {">

						<outline text="setPrefs ();"/>

						<outline text="getOutline (opmlUrl);"/>

						<outline text="});"/>

						</outline>

					<outline text="&lt;/script&gt;"/>

					</outline>

				</outline>

			</outline>

